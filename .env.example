# Example .env file for agent framework

# Required API keys
OPENAI_API_KEY=your-openai-api-key-here
GALILEO_API_KEY=your-galileo-api-key-here

# Required Galileo Console URL

GALILEO_PROJECT_ID=startup-simulator-v1.2
GALILEO_LOG_STREAM=my_log_stream


# Optional LLM configuration
LLM_MODEL=gpt-4
LLM_TEMPERATURE=0.7

# Optional agent configuration
VERBOSITY=low  # Options: none, low, high
ENVIRONMENT=development
ENABLE_LOGGING=true
ENABLE_TOOL_SELECTION=true
